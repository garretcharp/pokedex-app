{
  "name": "pokedex-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:local && conc npm:dev:next npm:dev:proxy",
    "dev:next": "next dev",
    "dev:proxy": "npx cf-bindings-proxy",
    "migrate:local": "npm run migrate -- --local",
    "migrate": "npx wrangler d1 migrations apply pokedex-prod",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages@1",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static"
  },
  "dependencies": {
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-separator": "^1.0.3",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "cf-bindings-proxy": "^0.2.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.11",
    "lucide-react": "^0.274.0",
    "next": "13.4.11",
    "postcss": "8.4.26",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.1.6",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.3.1",
    "@cloudflare/workers-types": "^4.20230717.1",
    "concurrently": "^8.2.0",
    "eslint-plugin-next-on-pages": "^1.3.1"
  }
}
